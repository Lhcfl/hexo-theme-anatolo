{
    "name": "hexo-theme-Anatolo",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "format:stylus": "stylus-supremacy format ./source/css/**/*.styl -r --options .supermacy.yml",
        "format:pretty": "prettier --write ./{includes,languages,layout,scripts,source,src}/**/*.{pug,js,css,ts,tsx,jsx}",
